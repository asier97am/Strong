<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asier.aranda.strong">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Strong1">

        <!-- Activity animacion splash screen -->
        <activity
            android:name=".Animation"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Activity registro -->
        <activity
            android:name=".Registro"
            android:exported="true"></activity> <!-- Activity bienvenida -->
        <activity
            android:name=".Bienvenida"
            android:exported="true">

        </activity>
    </application>

</manifest>